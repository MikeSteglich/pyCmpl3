%data : rollWidth , widths set, patterns set, orders[widths],  nbr[widths, patterns]

variables:
	 cut[patterns]: integer[0..];

objectives:
	number: sum{ j in patterns: cut[j] }->min;

constraints: 
	 {i in widths: fill[i]: sum{ j in patterns : nbr[i,j] * cut[j] } >= orders[i];} 

 
 
 
 
 
 
 
 
 
 
 
 
 
 
